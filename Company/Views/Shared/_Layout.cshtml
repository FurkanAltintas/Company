<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>Company - @ViewBag.Title</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="@Url.Content("~/Content/eBusiness/assets/img/favicon.png")" rel="icon">
    <link href="@Url.Content("~/Content/eBusiness/assets/img/apple-touch-icon.png")" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,400i,600,700|Raleway:300,400,400i,500,500i,700,800,900" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="@Url.Content("~/Content/eBusiness/assets/vendor/animate.css/animate.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/eBusiness/assets/vendor/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/eBusiness/assets/vendor/bootstrap-icons/bootstrap-icons.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/eBusiness/assets/vendor/boxicons/css/boxicons.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/eBusiness/assets/vendor/glightbox/css/glightbox.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/eBusiness/assets/vendor/swiper/swiper-bundle.min.css")" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="@Url.Content("~/Content/eBusiness/assets/css/style.css")" rel="stylesheet">

    <!-- =======================================================
    * Template Name: eBusiness - v4.3.0
    * Template URL: https://bootstrapmade.com/ebusiness-bootstrap-corporate-template/
    * Author: BootstrapMade.com
    * License: https://bootstrapmade.com/license/
    ======================================================== -->
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top d-flex align-items-center">
        <div class="container d-flex justify-content-between">

            <div class="logo">
                <h1><a href="@Url.Action("","")"><span>e</span>Business</a></h1>
                <!-- Uncomment below if you prefer to use an image logo -->
                <!-- <a href="index.html"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>-->
            </div>

            <nav id="navbar" class="navbar">
                <ul>
                    <li><a class="nav-link scrollto active" href="#hero">Home</a></li>
                    <li><a class="nav-link scrollto" href="#about">About</a></li>
                    <li><a class="nav-link scrollto" href="#services">Services</a></li>
                    <li><a class="nav-link scrollto" href="#team">Team</a></li>
                    <li><a class="nav-link scrollto" href="#working">Working With Us</a></li>
                    <li><a class="nav-link scrollto" href="#portfolio">Portfolio</a></li>
                    <li><a href="@Url.Action("Index","Blog")">Blog</a></li>
                    <li><a class="nav-link scrollto" href="#contact">Contact</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->

        </div>
    </header><!-- End Header -->
    <!-- ======= hero Section ======= -->
    @RenderBody()

    <div id="preloader"></div>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="@Url.Content("~/Content/eBusiness/assets/vendor/bootstrap/js/bootstrap.bundle.min.js")"></script>
    <script src="@Url.Content("~/Content/eBusiness/assets/vendor/glightbox/js/glightbox.min.js")"></script>
    <script src="@Url.Content("~/Content/eBusiness/assets/vendor/isotope-layout/isotope.pkgd.min.js")"></script>
    <script src="@Url.Content("~/Content/eBusiness/assets/vendor/php-email-form/validate.js")"></script>
    <script src="@Url.Content("~/Content/eBusiness/assets/vendor/swiper/swiper-bundle.min.js")"></script>

    <!-- Template Main JS File -->
    <script src="@Url.Content("~/Content/eBusiness/assets/js/main.js")"></script>

</body>

</html>